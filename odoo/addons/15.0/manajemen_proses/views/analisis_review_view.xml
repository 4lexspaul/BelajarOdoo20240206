<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_analisis_review" model="ir.actions.act_window">
        <field name="name">Analisis Review</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">manajemen.proses</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="context">{'default_metode':'kuantitatif','default_type':'high'}</field>
        <field name="domain">[('metode', '=', 'kualitatif'), ('type', '=', 'high')]</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create Your Frist Analisis Review !
            </p>
        </field>
    </record>

    <menuitem id="analisis_review"
              name="Analisis Review"
              action="action_analisis_review"
              parent="manajemen_proses_menu"
              sequence="20"/>
</odoo>
